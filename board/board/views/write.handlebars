<h1>[{{title}}]</h1>
<div>
	<form name="boardForm" method="post" action="/write">
		<div>
			<label>제목</label>
			<input
				type="text"
				name="title"
				placeholder="제목을 넣어주세요"
				value=""
			/>
		</div>
		<div>
			<label>이름</label>
			<input
				type="text"
				name="writer"
				placeholder="이름을 넣어주세요"
				value=""
			/>
		</div>
		<div>
			<label>비밀번호</label>
			<input
				type="password"
				name="password"
				placeholder="비밀번호를 넣어주세요"
				value=""
			/>
		</div>
		<div>
			<label>본문을 입력하세요</label>
			<textarea
				name="content"
				placeholder="본문을 넣어주세요"
				cols="50"
				rows="10"
			></textarea>
		</div>
		<div>
			<button type="submit">저장</button>
			<button type="button" onclick="localtion.href='/">취소</button>
		</div>
	</form>
</div>